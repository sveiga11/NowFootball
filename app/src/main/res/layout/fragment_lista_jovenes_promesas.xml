        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/viewVini"
        app:layout_constraintTop_toTopOf="@+id/viewVini"
        app:layout_constraintRight_toRightOf="@id/viewVini"
        app:layout_constraintLeft_toLeftOf="@id/viewVini"
        />
    <View
