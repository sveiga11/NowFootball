        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/musiala"
        app:layout_constraintBottom_toBottomOf="@+id/viewMusiala"
        app:layout_constraintTop_toTopOf="@+id/viewMusiala"
        app:layout_constraintStart_toStartOf="@id/viewMusiala"
        />

    <TextView
        android:id="@+id/textoMusiala"
