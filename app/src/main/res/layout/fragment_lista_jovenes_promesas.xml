<?xml version="1.0" encoding="utf-8"?>

<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:fillViewport="false">

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListaJovenesPromesas">

    <TextView
        android:id="@+id/titulo_lista"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAlignment="center"
        android:text="JOVENES PROMESAS"
        android:textSize="20dp"
        android:padding="20dp"
        android:layout_marginTop="20dp"
        android:drawableStart="@drawable/ic_baseline_diamond_24"
        android:drawableTint="#9D142F"
        app:layout_constraintBottom_toTopOf="@id/viewRodrygo"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textStyle="bold"
        android:textColor="#9D142F"
        />
    <View
        android:id="@+id/viewRodrygo"
        android:layout_width="match_parent"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_height="50dp"
        android:background="@drawable/border_radius"
        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/titulo_lista"
        app:layout_constraintBottom_toTopOf="@id/viewVini"
        />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageRodrygo"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/rodrygo"
        app:layout_constraintBottom_toBottomOf="@+id/viewRodrygo"
        app:layout_constraintTop_toTopOf="@+id/viewRodrygo"
        app:layout_constraintStart_toStartOf="@id/viewRodrygo"
        />

    <TextView
        android:id="@+id/textViewRodrygo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:text="RODRYGO"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/viewRodrygo"
        app:layout_constraintTop_toTopOf="@+id/viewRodrygo"
        app:layout_constraintRight_toRightOf="@id/viewRodrygo"
        app:layout_constraintLeft_toLeftOf="@id/viewRodrygo"
        />
    <View
        android:id="@+id/viewVini"
        android:layout_width="match_parent"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_height="50dp"
