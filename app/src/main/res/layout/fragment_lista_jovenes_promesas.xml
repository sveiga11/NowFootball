        android:layout_marginTop="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/viewRodrygo"
        app:layout_constraintBottom_toTopOf="@id/viewPedri"
        />

    <de.hdodenhof.circleimageview.CircleImageView
        android:id="@+id/imageVini"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_centerHorizontal="true"
        android:src="@drawable/vini"
        app:layout_constraintBottom_toBottomOf="@+id/viewVini"
        app:layout_constraintTop_toTopOf="@+id/viewVini"
        app:layout_constraintStart_toStartOf="@id/viewVini"
